<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.isp.faiaz.TrackRequestController">
    <Label layoutX="200.0" layoutY="14.0" style="-fx-font-size: 20px; -fx-font-weight: bold;" text="Track Request" />

    <TextField fx:id="tokenField" layoutX="141.0" layoutY="80.0" prefWidth="200" promptText="Enter Token Number" />
    <Label layoutX="33.0" layoutY="84.0" text="Token Number:" />

    <Button layoutX="371.0" layoutY="80.0" onAction="#handleTrackRequest" prefHeight="25.0" prefWidth="140.0" text="Track Status" />

    <Label fx:id="statusLabel" layoutX="150.0" layoutY="200.0" style="-fx-font-size: 16px; -fx-font-weight: bold;" />

    <Button layoutX="494.0" layoutY="320.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="38.0" prefWidth="60.0" text="Go Back" />

</AnchorPane>
