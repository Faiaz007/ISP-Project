<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="329.0" prefWidth="549.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.isp.fardeen.BillingInquiries">
    <Label layoutX="20" layoutY="20" text="Billing Inquiries" />
    <TableView fx:id="billingTableView" layoutX="20" layoutY="60" prefHeight="200" prefWidth="500">
        <columns>
            <TableColumn fx:id="inquiryIdTablceColumn" text="Inquiry ID" />
            <TableColumn fx:id="customerTableColumn" text="Customer" />
            <TableColumn fx:id="amountTableColumn" text="Amount" />
        </columns>
    </TableView>
    <Button layoutX="20" layoutY="280" onAction="#resolveInquiryOnAction" text="Resolve Inquiry" />
</AnchorPane>
